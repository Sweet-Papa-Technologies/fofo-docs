{
  "name": "fofo-docs",
  "version": "1.0.0",
  "description": "Tool to generate user documentation for code repositories.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:dev:bad-params": "npx ts-node src/index.ts",
    "test:dev:good-params": "npx ts-node src/index.ts fofo-docs --input ./test-data --output ./test-output --test true",
    "dev": "npx ts-node src/index.ts fofo-docs --input ./test-data --output ./test-output"
  },
  "author": "Forrester Terry (fterry@sweetpapatechnologies.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "commander": "^12.0.0",
    "glob": "^10.3.15"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
