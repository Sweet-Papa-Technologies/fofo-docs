{
  "name": "fofo-docs",
  "version": "1.0.0",
  "description": "Tool to generate user documentation for code repositories.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:dev:bad-params": "npx ts-node src/index.ts",
    "test:dev:good-params": "npx ts-node src/index.ts fofo-docs --input ./ --output ./test-output --test true",
    "test:docs:gen": "npx ts-node src/index.ts fofo-doc -g './sample-output/projectContext.json' --output ./test-output",
    "dev:single-file": "npx ts-node src/index.ts fofo-doc --input './src/shared.ts' --output ./test-output",
    "dev": "npx ts-node src/index.ts fofo-docs --input ./ --output ./test-output"
  },
  "author": "Forrester Terry (fterry@sweetpapatechnologies.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/aiplatform": "^3.20.0",
    "@google-cloud/vertexai": "^1.1.0",
    "@google/generative-ai": "^0.1.3",
    "chromadb": "^1.8.1",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "glob": "^10.3.15",
    "js-yaml": "^4.1.0",
    "jsonrepair": "^3.8.0",
    "ollama": "^0.5.1",
    "openai": "^4.47.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.4.5"
  }
}
